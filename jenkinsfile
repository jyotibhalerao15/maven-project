pipeline {
    agent any


    stages {
        stage('SCM Checkout'){
          git 'https://github.com/jyotibhalerao15/maven-project'
        }
  }
    {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'local-maven') {
                    sh 'mvn clean compile'
                }
            }
}
}
}
